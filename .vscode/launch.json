  "version": "0.2.0",
  "configurations": [
    {
      "name": "Save products of json",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "save_products",
        "${workspaceFolder}/products.json"
      ]
    },
    {
      "name": "Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "test"
      ],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Run server",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runserver",
        "8001"
      ],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "pgadmin4",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/venv/lib/python3.11/site-packages/pgadmin4/pgAdmin4.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
  ]
}